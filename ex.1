def sort(a):
    if len(a) > 1:
        m = len(a) // 2
        l = a[:m]
        r = a[m:]
        sort(l)
        sort(r)
        i = 0
        j = 0
        k = 0
        while i < len(l) and j < len(r):
            if l[i] < r[j]:
                a[k] = l[i]
                i = i + 1
            else:
                a[k] = r[j]
                j = j + 1
            k = k + 1
        while i < len(l):
            a[k] = l[i]
            i = i + 1
            k = k + 1
        while j < len(r):
            a[k] = r[j]
            j = j + 1
            k = k + 1
a = list(map(float, input().split()))
sort(a)
print(a)
